{
  "name": "xdsup_dex",
  "version": "0.1.0",
  "private": true,

  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "contract-build": "cargo build --target wasm32-unknown-unknown --release && npm run contract-postbuild",
    "contract-postbuild": "cp target/wasm32-unknown-unknown/release/xdsup_dex.wasm ./res/",
    "contract-test": "cargo test --package xdsup_dex -- --nocapture",
    "contract-deploy:dev": "npm run contract-build && near dev-deploy res/xdsup_dex.wasm",
    "contract-dev": "npm run contract-deploy:dev && npm start"
  },

  "dependencies": {
    "@types/big.js": "^6.1.3",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "big.js": "^6.1.1",
    "borsh": "^0.7.0",
    "buffer": "^6.0.3",
    "dotenv-expand": "^8.0.3",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "near-api-js": "^0.44.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "react-router": "^6.3.0",
    "react-select": "^5.3.2",
    "react-tabs": "^5.1.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.2"
  },

  "devDependencies": {
    "sass": "^1.37.0",
    "gh-pages": "^3.1.0"
  },

  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
